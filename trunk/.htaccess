Options FollowSymLinks

RewriteEngine On
RewriteRule ^$ index [L]

RewriteRule ^1,(.*),(.*),item.html$ index?p=$2&id=$1 [L]
RewriteRule ^1,(.*),3,(.*),1,quote.html$ index?p=3&id=$1&c=$2 [L]

RewriteRule ^category\.(.*)\.(.*)$ index?p=4&id=$1&start=$2 [L]
RewriteRule ^1,(.*),item.html$ index?p=$1&action=add [L]
RewriteRule ^index\.search$ index?p=search [L]
RewriteRule ^search\.(.*)\.(.*)$ index?p=search&search_word=$1&start=$2 [L]
RewriteRule ^index\.(.*)$ index?start=$1 [L]
RewriteRule ^2,(.*),item.html$ design?issue=$1 [L]
RewriteRule ^index$ index [L]
RewriteRule ^rss$ rss [L]
RewriteRule ^source\.(.*)$ source?id=$1 [L]

<FilesMatch ^[a-z]*$>
ForceType application/x-httpd-php
</FilesMatch>